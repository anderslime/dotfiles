[user]
	name = Anders Emil Nielsen
	email = aemilnielsen@gmail.com
[alias]
	ap = add -p
	cm = commit -m
	remove-merged = !sh -c 'git checkout master && git branch --merged | egrep -v \"production|master\" | xargs -L1 git delete-branch'
	tmp = commit -a -m 'tmp'
	wip = commit -a -m 'wip'
	co = checkout
	cob = checkout -b
	dc = diff --cached
	db = delete-branch
	ri = rebase --interactive --autosquash
	rim = rebase --interactive --autosquash master
[color]
	status = auto
	diff = auto
	branch = auto
[core]
	editor = vim --noplugin
	excludesfile = /Users/anderslime/.gitignore
[credential]
	helper = osxkeychain
[push]
	default = current
